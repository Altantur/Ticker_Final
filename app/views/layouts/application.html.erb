<!DOCTYPE html>
<html>
<head>
  <title>Ticker</title>
  <%= stylesheet_link_tag    "custom", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class='container'>
<% flash.each do |key,value| %>
	<div class="alert alert-<%=key %>"><%= value %></div>
<%end%>
<%= render 'layouts/header'%>
<%= yield %>
<%= render 'layouts/footer'%>
</div>

</body>
</html>