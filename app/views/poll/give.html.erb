<% unless params.keys[1] == "commit" %>
<% for nem in @count %>
<% if params.keys[1] == nem.value %>
<% a = Answer.find(nem.id) %>
<% temp = a.result.to_i %>
<% if  temp==nil 
	temp = 0 
	end %>
<% temp = temp + 1 %>
<% a.result = temp.to_s %>
<% a.save %>
<% end %>
<% end %>
<% end %>
<% @count = Answer.find_by_sql("select * from answers where poll_id = #{@poll.id}") %>
<%= render 'layouts/rss'%>